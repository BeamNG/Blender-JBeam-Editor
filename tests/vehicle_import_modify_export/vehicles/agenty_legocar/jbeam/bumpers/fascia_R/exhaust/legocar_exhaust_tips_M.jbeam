    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
	["lego_cyllinder_connector_talltoflat_black", ["legocar_exhaust_M"], [], {"pos":{"x": 0.000, "y":1.850, "z":0.000},"rot":{"x":-90, "y":0, "z":0},"scale":{"x":1, "y":1, "z":1}}],
    ],
    "nodes": [
        ["id", "posX", "posY", "posZ"],
	//add weight and change exhaust properties
