{
"legocar_exhausttip_L_black": {
    "information":{
        "name":"Left Exhaust Tip (Black)",
        "authors":"AgentY",
    },
    "slotType":"legocar_exhausttip_L",
    "slots": [
        ["type", "default", "description"],
	["legocar_afterfire_BL","legocar_afterfire_BL", "Afterfire Effect"],
	["legocar_nitrofire_BL","", "Nitrous Flames"],
    ],
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
	["lego_cyllinder_connector_talltoflat_black", ["legocar_exhaust_L"], [], {"pos":{"x": 0.500, "y":1.850, "z":0.000},"rot":{"x":-90, "y":0, "z":0},"scale":{"x":1, "y":1, "z":1}}],
    ],
    "nodes": [
        ["id", "posX", "posY", "posZ"],
	//add weight and change exhaust properties
	{"group":"legocar_exhaust_L"},
	//original 0.5 kg/node, + 1kg -> 1.5kg 
	{"nodeWeight": "$=$weight*1.5"},
	["Lexh3",  0.50, 1.80, 0.00],
	//original 0.5 kg/node, + 0.5kg -> 1kg 
	{"nodeWeight": "$=$weight*1"},
	["Lexh4",  0.50, 2.10, 0.00, {"afterFireAudioCoef":0.3,"afterFireVisualCoef":0.35, "exhaustAudioMufflingCoef": 0.6, "exhaustAudioGainChange":-2}],
    ],
},

