{
"legocar_electric_motor": {
    "information":{
        "name":"Customizable Electric Motor",
        "authors":"AgentY",
    },
    "slotType":"legocar_engine",
    "slots": [
        ["type", "default", "description"],
	["legocar_enginebay_electric","legocar_enginebay_electric_col2", "Engine Bay"],
	["legocar_transfercase_electric","legocar_transfercase_electric_AWD", "Transfer Case"],
    ],
    "variables": [
        ["name", "type", "unit", "category", "default", "min", "max", "title", "description"],
	["$inertia", "range", "kg*m^2", "Electric Motor",  0.10, 0.05, 0.50, "Inertia",      "How fast the motor builds up RPM",      {"stepDis":0.01}],
        ["$maxRPM",  "range", "RPM",    "Electric Motor",  10000,1000, 20000,"Max RPM",      "Max RPM at which the motor can operate",{"stepDis":100} ],
	["$torque",  "range", "Nm",     "Electric Motor",  300,  10,   1000, "Motor Torque", "Torque generated by the motor",         {"stepDis":10}  ],
    ],
    "powertrain": [
        ["type", "name", "inputName", "inputIndex"],
        ["electricMotor", "mainMotor", "dummy", 0],
    ],
    "mainMotor":{
        "torque":[
            ["rpm", "torque"],
	    [0, "$torque"],
            ["$maxRPM", "$torque"],
        ],
        "maxRPM":"$maxRPM",
        "inertia":"$inertia",
        "friction":0,
        "dynamicFriction":0,
        "electricalEfficiency":1,  
	"maximumWantedRegenTorque":"$torque",

        //node beam interface
        "torqueReactionNodes:":["motorrig","motorrig","motorrig"],
	"energyStorage": ["mainBattery", "rearBattery"],
        "waterDamage": {"[engineGroup]:":["motor_waterdamage"]},
        "engineBlock": {"[engineGroup]:":["motor_block"]},
        "breakTriggerBeam":"engine",
        "uiName":"Electric Motor",
        "soundConfig": "soundConfig",
    },
    "vehicleController": {
        "shiftLogicName":"electricMotor",
        "motorNames": ["mainMotor"],
        "topSpeedLimitReverse": -1,
        "defaultRegen": 0.5
        "brakeRegenCoef": 1
    },
    "soundConfig": {
        "sampleName": "ElectricMotor_02",
        "mainGain": -10,
    },
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
	["legocar_emotor",["legocar_emotor"], [], {"pos":{"x": 0.000, "y":-1.125, "z":0.4},"rot":{"x":0, "y":0, "z": 0 },"scale":{"x":0.99, "y":0.99, "z":0.99}}],
    ],   
    "nodes": [
        ["id", "posX", "posY", "posZ"],

	{"group":"legocar_emotor"},
	{"engineGroup":["motor_block","motor_waterdamage"]},

	//2 pieces, 5 nodes - 0.4 kg/node
	{"nodeWeight": "$=$weight*0.4"},
	
	["motor1l",  0.25, -1.00, 0.45],
	["motor1r", -0.25, -1.00, 0.45],
	["motor2l",  0.25, -1.25, 0.45],
	["motor2r", -0.25, -1.25, 0.45],	

	//rigidifier
	["motorrig", 0, -1.125, 0.55, {"collision":false, "selfCollision":false, "group":""}],
	{"engineGroup":""},
    ],
    "beams": [
        ["id1:", "id2:"],
        {"beamPrecompression":1, "beamType":"|NORMAL", "beamLongBound":1, "beamShortBound":1},
        {"beamDeform":"FLT_MAX", "beamStrength":"FLT_MAX"},
	{"breakGroup":""},

	//spring = weight * 1001000, damp = weight * 100
	//not counting rigidifier weight here
	{"beamSpring":"$=$weight*501000", "beamDamp":"$=$weight*50"},

	//uniform structure
	["motor1l", "motor1r"],
	["motor2l", "motor2r"],
	["motor1l", "motor2l"],
	["motor1r", "motor2r"],
	["motor1l", "motor2r"],
	["motor2l", "motor1r"],

	//rigidifier
	["motor1l", "motorrig"],
	["motor1r", "motorrig"],
	["motor2l", "motorrig"],
	["motor2r", "motorrig"],

	
	//--ATTACH--
	{"beamDeform":"$strength", "beamStrength":"$strength"},
	{"breakGroup":"engine_break"},
	{"beamSpring":"$=$weight*251000", "beamDamp":"$=$weight*25"},
	
	["motor1l", "engmount1l", {"name":"engine"}],
	["motor1l", "engmount1r"],
	["motor1l", "engmount2l"],
	["motor1l", "engmount2r"],

	["motor1r", "engmount1l"],
	["motor1r", "engmount1r"],
	["motor1r", "engmount2l"],
	["motor1r", "engmount2r"],

	["motor2l", "engmount1l"],
	["motor2l", "engmount1r"],
	["motor2l", "engmount2l"],
	["motor2l", "engmount2r"],

	["motor2r", "engmount1l"],
	["motor2r", "engmount1r"],
	["motor2r", "engmount2l"],
	["motor2r", "engmount2r"],
	
	{"breakGroup":""},


	//--DRIVESHAFT BREAK TRIGGER BEAMS--
	//activate driveshaft breaking when engine gets too far away from wheel axle mounts on chassis, don't affect physics
	{"beamPrecompression":1, "beamType":"|SUPPORT", "beamLongBound":0.2, "beamShortBound":1},
	{"beamSpring":0, "beamDamp":0},
	{"beamDeform":"FLT_MAX", "beamStrength":"FLT_MAX"},

	{"breakGroup":"driveshaft_F"},
	["motorrig","chassis2r", {"name":"driveshaft_F"}],
	["motorrig","chassis2l", {"name":"driveshaft_F"}],
	{"breakGroup":"driveshaft_R"},
	["motorrig","chassis7r", {"name":"driveshaft_R"}],
	["motorrig","chassis7l", {"name":"driveshaft_R"}],
	{"breakGroup":""},

	{"beamPrecompression":1, "beamType":"|NORMAL", "beamLongBound":1, "beamShortBound":1},
    ],
    "quads": [
        ["id1:","id2:","id3:","id4:"],
        {"group":"legocar_emotor"}, 
  	["motor1l","motor1r","motor2r","motor2l"],
        {"group":""},
    ],
},

}