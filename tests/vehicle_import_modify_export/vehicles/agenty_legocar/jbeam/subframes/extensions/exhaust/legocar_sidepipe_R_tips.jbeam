{
"legocar_exhausttip_side_R_black": {
    "information":{
        "name":"Right Exhaust Tip (Black)",
        "authors":"AgentY",
    },
    "slotType":"legocar_exhausttip_side_R",
    "slots": [
        ["type", "default", "description"],
	["legocar_afterfire_side_R","legocar_afterfire_side_R", "Afterfire Effect"],
	["legocar_nitrofire_side_R","", "Nitrous Flames"],
    ],
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
	["lego_cyllinder_connector_talltoflat_black", ["legocar_sidepipe_R"], [], {"pos":{"x":-0.900, "y":0.300, "z":0.18},"rot":{"x":-90, "y":0, "z":0},"scale":{"x":1, "y":1, "z":1}}],
    ],
    "nodes": [
        ["id", "posX", "posY", "posZ"],

	//add weight and change exhaust properties
	{"group":"legocar_sidepipe_R"},
	//original 0.67 kg/node, + 1kg -> 1.67kg 
	{"nodeWeight": "$=$weight*1.67"},
	["sidepipeR1",-0.90,-0.475, 0.18, {"selfCollision":false}],
	//original 0.67 kg/node, + 0.5kg -> 1.33kg 
	{"nodeWeight": "$=$weight*1.33"},
	["sidepipeR2",-0.90, 0.525, 0.18, {"afterFireAudioCoef":0.3,"afterFireVisualCoef":0.35, "exhaustAudioMufflingCoef": 0.6, "exhaustAudioGainChange":-2}],
    ],
},

