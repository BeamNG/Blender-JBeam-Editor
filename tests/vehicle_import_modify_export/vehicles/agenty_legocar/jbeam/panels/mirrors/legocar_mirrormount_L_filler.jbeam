	{"beamSpring":"$=$weight*251000", "beamDamp":"$=$weight*25"},
	{"beamDeform":"$strength", "beamStrength":"$strength"},
	{"breakGroup":"mirrormount_door_L_break"},
	{"optional":true},
	
	["mirrormt1l", "door1l"],
	["mirrormt2l", "door1l"],
	["mirrormt1ll", "door1l"],
	["mirrormt2ll", "door1l"],

	["mirrormt1l", "door2l"],
	["mirrormt2l", "door2l"],
	["mirrormt1ll", "door2l"],
	["mirrormt2ll", "door2l"],
	
	{"breakGroup":""},
	{"optional":false},

	{"beamPrecompression":1, "beamType":"|NORMAL", "beamLongBound":1, "beamShortBound":1},
    ],
    "torsionHydros": [
        ["id1:","id2:","id3:","id4:"],
	//spring = weight * 50000, damp = weight * 5
        {"spring":"$=$weight*51000", "damp":"$=$weight*5", "deform":1000, "strength":"FLT_MAX"},
	//door opening
	//factor: 0.375*pi
        ["door4l","door2l","door1l","mirrormt1ll", {"factor":-1.1780972450961724644234912687298,"inRate":2,"outRate":2,"inputSource":"doorFL", "optional":true}],
